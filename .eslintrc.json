{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": "airbnb-base",
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        // "no-console": "off",
        "semi": ["error", "always"],
        "quotes": ["error", "double"],
        "import/extensions": ["error", "ignorePackages"],
        "import/no-unresolved": "off",
        "import/prefer-default-export": "off",
        "no-plusplus": "off"
    }
}
